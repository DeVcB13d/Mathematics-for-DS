%V = input("Enter a matrix : ");
V = [2,1,0,-1;1,0,2,-1;0,-2,1,0];
V = V';
[m,n] = size(V);
V1 = zeros(m,n);
V2 = zeros(m,n);
for i = 1:n
    r = V(:,i);
    for j = 1:i-1
        %Subtracting the dot pdt of rows before it
        f = sum(V(:,i).*V(:,j))/(sum(V(:,i).*V(:,i)));
        r = r - f*V1(:,j);
    end
    V1(:,i) = r;
    V2(:,i) = sqrt(sum(V1(:,)))
end




